local function toggleInfiniteJump()
    InfiniteJumpEnabled = not InfiniteJumpEnabled
    if InfiniteJumpEnabled then
        button.Text = "Close Infinite Jump"
    else
        button.Text = "Open Infinite Jump"
    end
end

