    local StarterGui = game:GetService("StarterGui")
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = description,
        Button1 = buttonText,
        Icon = "rbxthumb://type=AvatarHeadShot&id=5496251247&w=48&h=48",
        Duration = duration
    })
end

