    isMinimized = not isMinimized
    if isMinimized then
        previousSize = Frame.Size
        Frame.Size = UDim2.new(0, 350, 0, 30)
    else
        Frame.Size = previousSize
    end
    for _, child in ipairs(Frame:GetChildren()) do
        if child ~= DragBar then
            child.Visible = not isMinimized
        end
    end
