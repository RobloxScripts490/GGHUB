ScriptBox.MultiLine = true
ScriptBox.TextWrapped = false
ScriptBox.TextXAlignment = Enum.TextXAlignment.Left
ScriptBox.TextYAlignment = Enum.TextYAlignment.Top
ScriptBox.Parent = ScrollingFrame

